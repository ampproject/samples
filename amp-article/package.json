{
  "name": "amp-article",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -r @babel/register -w src/ src/server/index.js",
    "build": "babel src/ -d dist/",
    "postinstall": "npm run build",
    "start": "node dist/server",
    "test": "npm run test:lint && npm run test:amplint",
    "test:fix": "npm run test:lint -- --fix",
    "test:lint": "./node_modules/.bin/eslint .",
    "test:amplint": "WEB_URL=http://localhost node -r @babel/register ./test/amp.js"
  },
  "dependencies": {
    "@babel/preset-react": "7.0.0",
    "clean-css": "4.2.1",
    "midori": "3.3.0",
    "node-fetch": "2.3.0",
    "ramda": "0.26.1",
    "random": "2.0.13",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-feather": "1.1.5",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "string-hash": "1.1.3",
    "styled-components": "4.1.2",
    "url-join": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/preset-env": "7.2.3",
    "@babel/register": "7.0.0",
    "amphtml-validator": "1.0.23",
    "babel-plugin-module-resolver": "3.1.1",
    "eslint": "5.10.0",
    "eslint-config-metalab": "10.0.0",
    "nodemon": "1.18.8",
    "prettier": "1.15.3"
  }
}
