{
  "name": "amp-article",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -r @babel/register -w src/ src/server/index.js",
    "build": "npm run build:server && npm run build:static",
    "build:server": "babel src/ -d build/",
    "build:static": "node -r @babel/register ./task/build_static.js",
    "postinstall": "npm run build",
    "start": "npm run start:static",
    "start:static": "npx http-server dist/",
    "start:server": "node build/server",
    "test": "npm run test:lint && npm run test:amplint",
    "test:fix": "npm run test:lint -- --fix",
    "test:lint": "./node_modules/.bin/eslint .",
    "test:amplint": "MINIFY_MARKUP=true WEB_URL=http://localhost node -r @babel/register ./test/amp.js"
  },
  "dependencies": {
    "@babel/preset-react": "7.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "clean-css": "4.2.1",
    "http-server": "^0.11.1",
    "js-beautify": "^1.10.2",
    "midori": "3.3.0",
    "moize": "^5.4.4",
    "node-fetch": "2.6.1",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "ramda": "0.26.1",
    "random": "2.0.12",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-feather": "1.1.4",
    "react-helmet": "^5.2.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "string-hash": "1.1.3",
    "styled-components": "4.1.2",
    "url-join": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/preset-env": "7.2.0",
    "@babel/register": "7.0.0",
    "amphtml-validator": "1.0.23",
    "babel-plugin-module-resolver": "3.1.1",
    "copy-dir": "^1.2.0",
    "eslint": "5.10.0",
    "eslint-config-metalab": "10.0.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "rimraf": "^3.0.0"
  }
}
